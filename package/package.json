{
  "name": "@gfazioli/mantine-compare",
  "version": "1.0.0",
  "description": "A Mantine extension component that provides a clean, flexible frame for showcasing before/after states, light vs dark themes, or alternative designs with support for keyboard navigation and ARIA labeling.",
  "homepage": "https://gfazioli.github.io/mantine-compare/",
  "packageManager": "yarn@4.0.1",
  "license": "MIT",
  "author": "Giovambattista Fazioli <giovambattista.fazioli@gmail.com>",
  "keywords": [
    "extension",
    "compare",
    "comparison",
    "before-after",
    "mantine",
    "react",
    "react-component",
    "typescript",
    "accessibility",
    "a11y"
  ],
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.mts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./styles.css": "./dist/styles.css",
    "./styles.layer.css": "./dist/styles.layer.css"
  },
  "repository": "https://github.com/gfazioli/mantine-compare.git",
  "peerDependencies": {
    "@mantine/core": ">=7.0.0",
    "@mantine/hooks": ">=7.0.0",
    "@tabler/icons-react": "^3.34.0",
    "react": "^18.x || ^19.x",
    "react-dom": "^18.x || ^19.x"
  },
  "bugs": "https://github.com/gfazioli/mantine-compare/issues"
}
